{
  "manifest_version": 2,
  "name": "Filter Challenges",
  "version": "1.2.0",
  "description": "Filters Completed Challenges and Projects on Microverse's Student Dashboard",
  "permissions": [
    "tabs",
    "*://dashboard.microverse.org/*",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://dashboard.microverse.org/*"
      ],
      "js": [
        "filter.js"
      ]
    }
  ]
}